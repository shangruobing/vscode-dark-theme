{
	"name": "Jetbrains New UI Theme",
	"type": "dark",
	"colors": {
		"activityBar.background": "#2b2d30",
		"activityBarBadge.background": "#007acc",
		"breadcrumb.background": "#2b2d30",
		"editor.background": "#1e1f22",
		"editor.foreground": "#d4d4d4",
		"editor.inactiveSelectionBackground": "#3a3d41",
		"editor.lineHighlightBackground": "#2b2d30",
		"editor.selectionHighlightBackground": "#add6ff26",
		"editorBracketHighlight.foreground1": "#a9b7c6",
		"editorBracketHighlight.foreground2": "#a9b7c6",
		"editorBracketHighlight.foreground3": "#a9b7c6",
		"editorBracketHighlight.foreground4": "#a9b7c6",
		"editorBracketHighlight.foreground5": "#a9b7c6",
		"editorBracketHighlight.foreground6": "#a9b7c6",
		"editorGroupHeader.border": "#393b40",
		"editorGroupHeader.tabsBackground": "#1e1f22",
		"editorGutter.background": "#1e1f22",
		"editorIndentGuide.activeBackground": "#707070",
		"editorIndentGuide.background": "#404040",
		"input.placeholderForeground": "#a6a6a6",
		"list.activeSelectionIconForeground": "#ffffff",
		"list.dropBackground": "#383b3d",
		"menu.background": "#2b2d30",
		"menu.foreground": "#cccccc",
		"ports.iconRunningProcessForeground": "#369432",
		"sideBar.background": "#2b2d30",
		"sideBarSectionHeader.background": "#2b2d30",
		"sideBarSectionHeader.border": "#cccccc33",
		"sideBarTitle.foreground": "#bbbbbb",
		"statusBar.background": "#2b2d30",
		"statusBar.noFolderBackground": "#2b2d30",
		"statusBarItem.remoteBackground": "#16825d",
		"statusBarItem.remoteForeground": "#ffffff",
		"tab.activeBackground": "#1e1f22",
		"tab.activeBorder": "#3573f0",
		"tab.activeBorderTop": "#1e1f22",
		"tab.border": "#1e1f22",
		"tab.inactiveBackground": "#1e1f22",
		"tab.lastPinnedBorder": "#cccccc33",
		"terminal.inactiveSelectionBackground": "#3a3d41",
		"titleBar.activeBackground": "#2b2d30",
	},
	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"foreground": "#6A8759"
			}
		},
		{
			"scope": "punctuation.definition.comment",
			"settings": {
				"foreground": "#6A8759"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#6A8759"
			}
		},
		{
			"scope": "meta.embedded.assembly",
			"settings": {
				"foreground": "#6A8759"
			}
		},
		{
			"scope": "keyword - keyword.operator",
			"settings": {
				"foreground": "#CC7832"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#CC7832"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#CC7832"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#CC7832"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#6897BB"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#FFC66D"
			}
		},
		{
			"scope": "support.function",
			"settings": {
				"foreground": "#FFC66D"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#A9B7C6"
			}
		},
		{
			"scope": "entity.name.variable",
			"settings": {
				"foreground": "#A9B7C6"
			}
		},
		{
			"scope": [
				"comment",
				"constant.other.enum.java"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"entity.name.type.class",
				"variable.parameter",
				"punctuation",
				"keyword.operator.assignment",
				"keyword.operator",
				"source.python",
				"meta",
				"entity.other.attribute-name",
				"variable.other.readwrite",
				"support.type.property-name",
				"entity.name.function",
				"entity.name.section.markdown",
				"punctuation.definition.list.begin.markdown",
				"storage.modifier.import.java",
				"entity.name.type.enum.java",
				"storage.modifier.package.java",
				"support",
				"constant.language.import-export-all.ts",
				"storage.type.java"
			],
			"settings": {
				"foreground": "#A9B7C6"
			}
		},
		{
			"scope": [
				"entity.name.tag",
				"entity.other.attribute-name.class.css",
				"punctuation.definition.tag",
				"entity.name.tag.css",
				"entity.other.attribute-name.scss",
				"entity.other.attribute-name.pseudo-class.css",
				"entity.other.attribute-name.id.css",
				"entity.name.function.ts",
				"entity.name.function.vue",
				"keyword.other.important.scss",
				"punctuation.definition.entity.css",
				"meta.property-value.scss",
				"entity.name.function.scss"
			],
			"settings": {
				"foreground": "#FFC66D"
			}
		},
		{
			"scope": [
				"constant.language.boolean",
				"storage.modifier",
				"keyword.operator.new",
				"constant.language.null",
				"constant.character.escape",
				"constant.language.python",
				"keyword.other.import.java",
				"keyword.other.package.java",
				"constant.language.json",
				"punctuation.definition.keyword.scss",
				"keyword.operator.logical.python",
				"variable.language.this.java",
				"constant.language.java"
			],
			"settings": {
				"foreground": "#CC7832"
			}
		},
		{
			"scope": [
				"support.constant.property-value.css",
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "#A5C261"
			}
		},
		{
			"scope": [
				"comment.block.javadoc.java",
				"keyword.other.documentation.javadoc.java",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#6A8759"
			}
		},
		{
			"scope": [
				"variable.other.property",
				"constant.other.color.rgb-value.hex.css",
				"variable.other.object.property.java",
				"constant.other.enum.java",
				"meta.object-literal.key.ts",
				"variable.scss",
				"variable.language.special.self.python",
				"variable.parameter.function.language.special.self.python",
				"variable.other.definition.java",
				"variable.other.object.property.ts"
			],
			"settings": {
				"foreground": "#9E7BB0"
			}
		},
		{
			"scope": [
				"support.type.primitive",
				"entity.name.type"
			],
			"settings": {
				"foreground": "#7A9EC2"
			}
		},
		{
			"scope": [
				"storage.type.annotation.java",
				"punctuation.definition.annotation.java"
			],
			"settings": {
				"foreground": "#BBB529"
			}
		},
	]
}