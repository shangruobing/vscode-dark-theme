{
  "name": "vscode-dark-theme",
  "displayName": "JetBrains Dark Theme",
  "description": "A Visual Studio Code theme extension based on the JetBrains IDEs Dark theme.",
  "version": "0.0.16",
  "author": "Ruobing Shang",
  "publisher": "Ruobing Shang",
  "repository": "https://github.com/shangruobing/vscode-dark-theme",
  "icon": "jetbrains.png",
  "engines": {
    "vscode": "^1.50.0"
  },
  "scripts": {
    "install vsce": "npm install vsce -g --save vsce",
    "build": "vsce package",
    "publish": "vsce publish"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "JetBrains Dark Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/dark.json"
      }
    ]
  }
}
